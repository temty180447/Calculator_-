private: System::Void btncalculate_Click(System::Object^ sender, System::EventArgs^ e) {
	String^ text = textBox1->Text;

	if (!String::IsNullOrEmpty(text)) {

		if (text->Contains("e^")) {
			int index = text->IndexOf("e^") + 2; // เพิ่ม 2 เพื่อให้ index เริ่มต้นที่ตัวเลขที่ต้องการยกกำลัง
			String^ exponentString = text->Substring(index); // เลือกส่วนของข้อความที่เป็นที่ยกกำลัง
			double exponent = Double::Parse(exponentString); // แปลงเป็นเลขที่จะใช้ในการยกกำลัง

			double result = Math::Pow(2.71828, exponent); // คำนวณค่า e^exponent

			MessageBox::Show(L" ผลลัพธ์ " + result);
		}
		else if (text->Contains("^")) {
			array<String^>^ parts = text->Split('^');

			if (parts->Length == 2) {
				double baseNumber = Convert::ToDouble(parts[0]);
				double exponent = Convert::ToDouble(parts[1]);

				double result = Math::Pow(baseNumber, exponent);

				String^ resultMessage = L"เลขฐาน: " + baseNumber.ToString() + L"\nเลขยกกำลัง: " + exponent.ToString() + L"\nผลลัพธ์: " + result.ToString();
				MessageBox::Show(resultMessage);

				textBox1->Text = result.ToString();
			}
		}
		else if (text->Contains("!")) {
			int index = text->IndexOf("!");
			String^ numberString = text->Substring(0, index);
			int number = Int32::Parse(numberString);

			int result = 1;
			for (int i = number; i >= 1; i--) {
				result *= i;
			}

			MessageBox::Show(L" ผลลัพธ์ " + result);
		}
		else if (text->Contains("/")) {
			array<String^>^ parts = text->Split('/');

			if (parts->Length == 2) {
				double fractionPart = Convert::ToDouble(parts[0]);
				double wholePart = Convert::ToDouble(parts[1]);

				if (wholePart != 0) {
					double result = fractionPart / wholePart;

					String^ resultMessage = L"เศษ: " + fractionPart.ToString() + L"\nส่วน: " + wholePart.ToString() + L"\nผลลัพธ์: " + result.ToString();
					MessageBox::Show(resultMessage);

					textBox1->Text = result.ToString();
				}
				else {
					MessageBox::Show(L"ส่วนตัวหารต้องไม่เป็นศูนย์");
				}
			}
		}
		else if (text->Contains("log(") && text->Contains(")")) {
			// ค้นหาตำแหน่งของสัญลักษณ์วงเล็บแรกและสุดท้าย
			int startIndex = text->IndexOf("log(") + 4; // เพิ่ม 4 เพื่อข้ามตัวอักษร "log("
			int endIndex = text->LastIndexOf(')');

			// ตรวจสอบว่ามีสัญลักษณ์วงเล็บทั้งสองตัวหรือไม่
			if (endIndex > startIndex) {
				// แยกฐานและค่า log
				String^ baseLog = text->Substring(startIndex, endIndex - startIndex);
				String^ valueLog = text->Substring(endIndex + 1); // เพิ่มการข้ามเครื่องหมาย ")" 

				// แปลงค่าฐานและค่า log เป็น double
				double baseLogValue = Convert::ToDouble(baseLog);
				double valueLogValue = Convert::ToDouble(valueLog);

				// คำนวณค่า log
				double result = log(valueLogValue) / log(baseLogValue); // ใช้ฟังก์ชัน log() เพื่อคำนวณ log ฐาน 10

				// แสดงผลลัพธ์ใน MessageBox
				String^ resultMessage = L"ฐาน: " + baseLog + L"\nค่า log: " + valueLog + L"\nผลลัพธ์: " + result.ToString();
				MessageBox::Show(resultMessage);

				// แสดงผลลัพธ์ใน textBox1
				textBox1->Text = result.ToString();
			}
		}
		else if (text->Contains("(") && text->Contains(")")) {
			// ค้นหาตำแหน่งของสัญลักษณ์วงเล็บแรกและสุดท้าย
			int startIndex = text->IndexOf('(');
			int endIndex = text->LastIndexOf(')');

			// ตรวจสอบว่ามีสัญลักษณ์วงเล็บทั้งสองตัวหรือไม่
			if (endIndex > startIndex) {
				// แยกเลขรากและค่าในราก
				String^ baseRoot = text->Substring(startIndex + 1, endIndex - startIndex - 1);
				String^ valueInRoot = text->Substring(endIndex + 2); // เพิ่มการข้ามเครื่องหมาย √

				// แปลงค่าเลขรากและค่าในรากเป็น double
				double baseRootValue = Convert::ToDouble(baseRoot);
				double valueInRootValue = Convert::ToDouble(valueInRoot);

				// คำนวณค่าในราก
				double result = Math::Pow(valueInRootValue, 1.0 / baseRootValue);

				// แสดงผลลัพธ์ใน MessageBox
				String^ resultMessage = L"รากที่ : " + baseRoot + L"\nของ: " + valueInRoot + L"\nผลลัพธ์: " + result.ToString();
				MessageBox::Show(resultMessage);

				// แสดงผลลัพธ์ใน textBox1
				textBox1->Text = result.ToString();
			}
		}
		else if (text->Contains(L"√")) {
			int index = text->IndexOf(L"√");
			String^ numberString = text->Substring(index + 1); // เอาตั้งแต่ตัวเลขที่ติดตัว √
			double number = Double::Parse(numberString);

			double result = sqrt(number); // คำนวณรากที่สอง

			MessageBox::Show(L" ผลลัพธ์ " + result);
			textBox1->Text = result.ToString();
		}
	}
	
}
private: System::Void btnfraction_Click(System::Object^ sender, System::EventArgs^ e) {
		   if (textBox1->Text == "0") {
			   // เคลียจอ
			   textBox1->Clear();
		   }
		   textBox1->Text += "/";
}
