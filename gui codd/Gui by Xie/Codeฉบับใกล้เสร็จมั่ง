private: System::Void btndriftingx_Click(System::Object^ sender, System::EventArgs^ e) {
	String^ text = textBox1->Text;
	if (text->Contains("log(") && text->Contains(")")) {
		// ค้นหาตำแหน่งของสัญลักษณ์วงเล็บแรกและสุดท้าย
		int startIndex = text->IndexOf("log(") + 4; // เพิ่ม 4 เพื่อข้ามตัวอักษร "log("
		int endIndex = text->LastIndexOf(')');

		// ตรวจสอบว่ามีสัญลักษณ์วงเล็บทั้งสองตัวหรือไม่
		if (endIndex > startIndex) {
			// แยกฐานและค่า log
			String^ baseLog = text->Substring(startIndex, endIndex - startIndex);
			String^ valueLog = text->Substring(endIndex + 1); // เพิ่มการข้ามเครื่องหมาย ")" 

			// แปลงค่าฐานและค่า log เป็น double
			double baseLogValue = Convert::ToDouble(baseLog);
			double valueLogValue = Convert::ToDouble(valueLog);

			// คำนวณค่า log
			double result = log(valueLogValue) / log(baseLogValue); // ใช้ฟังก์ชัน log() เพื่อคำนวณ log ฐาน 10

			// แสดงผลลัพธ์ใน MessageBox
			String^ resultMessage = L"ฐาน: " + baseLog + L"\nค่า log: " + valueLog + L"\nผลลัพธ์: " + result.ToString();
			MessageBox::Show(resultMessage);

			// แสดงผลลัพธ์ใน textBox1
			textBox1->Text = result.ToString();
		}
		else {
			MessageBox::Show(L"โปรดป้อนค่าให้ถูกต้องในรูปแบบ 'log(ฐาน)ค่า log'");
		}
	}
}
private: System::Void btneex_Click(System::Object^ sender, System::EventArgs^ e) {
	if (textBox1->Text == "0") {
		textBox1->Clear();
	}
	textBox1->Text += "e^";
}
private: System::Void btnlogT_Click(System::Object^ sender, System::EventArgs^ e) {
	if (textBox1->Text == "0") {
		textBox1->Clear();
	}
	textBox1->Text += "log";
	textBox1->Text += "(";
	textBox1->Text += ")";
}
